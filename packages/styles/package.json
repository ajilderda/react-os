{
  "name": "@desktop-ui/styles",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist/global.css",
    "dist/style.css"
  ],
  "scripts": {
    "vite": "vite build --mode global",
    "dev": "concurrently \"pnpm build:types --w\" \"pnpm storybook\"",
    "storybook": "storybook dev -p 6006",
    "dev:css": "postcss src/style.module.css -o dist/styles.css -w",
    "dev:css:globalVars": "postcss src/global-variables.css -o dist/global-variables.css -w",
    "build": "rm -rf dist && pnpm build:types && vite build && vite build --mode global && storybook build",
    "build:css": "postcss src/style.module.css -o dist/styles.css",
    "build:css:globalVars": "postcss src/global-variables.css -o dist/global-variables.css",
    "build:types": "tcm src",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@csstools/postcss-design-tokens": "^2.0.2",
    "@daltontan/postcss-import-json": "^1.1.1",
    "@storybook/addon-essentials": "^7.0.0-beta.50",
    "@storybook/addon-interactions": "^7.0.0-beta.50",
    "@storybook/addon-links": "^7.0.0-beta.50",
    "@storybook/blocks": "^7.0.0-beta.50",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/web-components": "^7.0.0-beta.50",
    "@storybook/web-components-vite": "^7.0.0-beta.50",
    "clsx": "^1.2.1",
    "concurrently": "^7.6.0",
    "cssnano": "^5.1.14",
    "eslint-config-custom": "workspace:*",
    "lit": "^2.6.1",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-dark-theme-class": "^0.7.3",
    "postcss-import": "^15.1.0",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.0",
    "postcss-preset-env": "^8.0.1",
    "postcss-scss": "^4.0.6",
    "postcss-simple-vars": "^7.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.50",
    "tsconfig": "workspace:*",
    "typed-css-modules": "^0.7.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "lit-html": "^2.6.1"
  }
}
