    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.50",
    "tsconfig": "workspace:*",
    "typed-css-modules": "^0.7.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "lit-html": "^2.6.1"
