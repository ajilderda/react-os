{
  "name": "@desktop-ui/styles",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "scripts": {
    "build:types": "tcm -o src dist/global",
    "build": "rm -rf dist && vite build && vite build --mode module && storybook build  && pnpm build:types",
    "dev": "concurrently \"pnpm build:types --w\" \"pnpm storybook\"",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6008 --no-open",
    "vite": "vite build --mode module"
  },
  "devDependencies": {
    "@desktop-ui/postcss-config": "workspace:*",
    "@desktop-ui/storybook-config": "workspace:*",
    "@desktop-ui/tokens": "workspace:*",
    "@desktop-ui/vite-config": "workspace:*",
    "@daltontan/postcss-import-json": "^1.1.1",
    "@storybook/addon-essentials": "^7.0.0-beta.50",
    "@storybook/addon-interactions": "^7.0.0-beta.50",
    "@storybook/addon-links": "^7.0.0-beta.50",
    "@storybook/blocks": "^7.0.0-beta.50",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/web-components": "^7.0.0-beta.50",
    "@storybook/web-components-vite": "^7.0.0-beta.50",
    "clsx": "^1.2.1",
    "concurrently": "^7.6.0",
    "cssnano": "^5.1.14",
    "eslint-config-custom": "workspace:*",
    "lit": "^2.6.1",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-dark-theme-class": "^0.7.3",
